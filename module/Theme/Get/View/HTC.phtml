<?php
header ( 'Cache-Control: no-cache, must-revalidate' );
header ( 'Expires: ' . $this->expire );
header ( 'Content-Type: text/x-component' );
$cache = $this->cache;
if (! $cache->start ( $this->key )) {
	$filepath = $this->htc;
	if (file_exists ( $filepath )) {
		include $filepath;
	}
	$cache->end ();
}
